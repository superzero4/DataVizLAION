<script>
function validateOnlyTextField(element) {
    var str = element.value;
    if(str == ' '){
        element.value = "";
    }
    if (!(/^[a-zA-Z\s]+$/.test(str))) {
        // console.log('String contain number characters');
        str = str.substr(0, str.length - 1);
        element.value = "";
    }
}
